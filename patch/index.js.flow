//@flow
// Patch operations based on json-merge-patch

type PatchOperation<T>
    = (set:SetOperation<T>, unset:UnsetOperation<T>) =>
        (el:T) => (key:string) => (data:{}) => T

type SetOperation<T> = (el:T) => (value:mixed) => T

type UnsetOperation<T> = (el:T) =>() => T

declare export var patchElement : PatchOperation<Element>
